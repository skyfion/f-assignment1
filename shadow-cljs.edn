;; shadow-cljs configuration
{:source-paths
 ["src/main"]

 :dependencies
 [[reagent "0.9.1"]
  [re-frame "0.12.0"]
  [binaryage/devtools "0.9.10"]]

 :nrepl        {:port 3333}

 :builds
 {:app
  {:target :browser
   :modules {:main {:init-fn core/init}}
   :output-dir "public/js"
   :asset-path "/js"
   :devtools {:http-root   "public"
              :http-port   3000}}}}

